{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "plugins": ["babel", "react","react-hooks","legacy-decorators", "prefer-class-properties", "no-copy-paste-default-export"],
  "extends": ["eslint:recommended", "babel", "react"],
  "rules": {
    "camelcase": 0,
    "comma-dangle": "warn",
    "consistent-this": "warn",
    "eqeqeq": 0,
    "indent": ["warn", 2],
    "keyword-spacing": 0,
    "max-len": [2, { "code": 16384, "tabWidth": 2, "ignoreUrls": true }],
    "new-cap": 0,
    "no-confusing-arrow": "warn",
    "no-console": 0,
    "no-dupe-keys": 1,
    "no-empty": "warn",
    "no-extra-parens": 0,
    "no-nested-ternary": "warn",
    "no-undef": 1,
    "no-unexpected-multiline": "warn",
    "no-unused-expressions": 1,
    "no-unused-vars": "warn",
    "prefer-reflect": 1,
    "radix": "warn",
    "space-before-function-paren": 0,
    "use-isnan": 1,
    "react/display-name": 0,
    "react/forbid-prop-types": 1,
    "react/jsx-boolean-value": 1,
    "react/jsx-no-duplicate-props": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-uses-vars": 1,
    "react/jsx-no-bind": [
      1,
      {
        "ignoreDOMComponents": false,
        "ignoreRefs": false,
        "allowArrowFunctions": false,
        "allowFunctions": false,
        "allowBind": false
      }
    ],
    "react/no-direct-mutation-state": 1,
    "react/no-unknown-property": 1,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "react/self-closing-comp": 1,
    "react/sort-comp": 1,
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  },
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.10.2"
    }
  }
}
